{
    "version": 2,
    "builds": [
      { "src": "ecommercePrj/wsgi.py", "use": "@vercel/python"
    , "config": { "maxLambdaSize": "15mb" ,"runtime": "python3.9" }
    }
      
    ],
    "routes": [
      { "src": "/(.*)", "dest": "ecommercePrj/wsgi.py" },
      { "src": "/static/(.*)", "dest": "/static/$1" }
    ]
  }
